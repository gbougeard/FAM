<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sun-web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Application Server 9.0 Servlet 2.5//EN"
        "http://www.sun.com/software/appserver/dtds/sun-web-app_2_5-0.dtd">
<sun-web-app error-url="">

    <context-root>/FAM-admin</context-root>
    <security-role-mapping>
        <role-name>Administrator</role-name>
        <group-name>ADMIN</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>Manager</role-name>
        <group-name>Managers</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>Guest</role-name>
        <group-name>Guests</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>Coach</role-name>
        <group-name>COACH</group-name>
    </security-role-mapping>
    <security-role-mapping>
        <role-name>Player</role-name>
        <group-name>PLAYER</group-name>
    </security-role-mapping>

    <class-loader delegate="true"/>
    <jsp-config>
        <property name="keepgenerated" value="true">
            <description>Keep a copy of the generated servlet class java code.</description>
        </property>
    </jsp-config>
</sun-web-app>